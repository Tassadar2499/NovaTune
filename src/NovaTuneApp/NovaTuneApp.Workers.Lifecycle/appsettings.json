{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "KafkaFlow": "Warning"
    }
  },
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "TopicPrefix": "dev"
  },
  "Lifecycle": {
    "PollingInterval": "00:05:00",
    "BatchSize": 50,
    "MaxConcurrency": 10,
    "RetryDelay": "00:01:00",
    "MaxRetryAttempts": 3
  },
  "MinIO": {
    "Endpoint": "http://localhost:9000",
    "AccessKey": "minioadmin",
    "SecretKey": "minioadmin",
    "BucketName": "novatune-audio"
  },
  "RavenDb": {
    "Url": "http://localhost:8080",
    "Database": "NovaTune"
  }
}
