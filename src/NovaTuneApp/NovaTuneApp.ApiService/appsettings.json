{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "cache": "localhost:6379"
  },
  "Cache": {
    "PresignedUrlTtlMinutes": 8,
    "SessionTtlMinutes": 30,
    "KeyPrefix": "novatune:"
  },
  "Kafka": {
    "BootstrapServers": "localhost:19092",
    "TopicPrefix": "prod",
    "SaslEnabled": false,
    "SaslUsername": "",
    "SaslPassword": ""
  }
}
