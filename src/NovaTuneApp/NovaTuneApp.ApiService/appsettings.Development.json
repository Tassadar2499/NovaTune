{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "cache": "localhost:6379"
  },
  "Cache": {
    "PresignedUrlTtlMinutes": 8,
    "SessionTtlMinutes": 30,
    "KeyPrefix": "novatune:dev:"
  },
  "Kafka": {
    "BootstrapServers": "127.0.0.1:19092",
    "TopicPrefix": "dev",
    "SaslEnabled": false,
    "SaslUsername": "",
    "SaslPassword": ""
  },
  "NovaTune": {
    "TopicPrefix": "dev",
    "PresignedUrl": {
      "TtlSeconds": 300
    },
    "CacheEncryption": {
      "Enabled": false
    },
    "RateLimit": {
      "RequestsPerMinute": 100,
      "AnonymousRequestsPerMinute": 20,
      "UploadsPerHour": 10
    },
    "Quotas": {
      "MaxUploadSizeBytes": 104857600,
      "MaxPlaylistsPerUser": 50,
      "MaxTracksPerPlaylist": 500,
      "MaxStoragePerUserBytes": 10737418240
    }
  }
}
